{
    "plugins": ["jest"],
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": ["airbnb-base", "plugin:jest/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "class-methods-use-this": "off",
        "comma-dangle": "off",
        "object-curly-newline": "off",
        "operator-linebreak": "off",
        "no-restricted-syntax": "off"
    }
}
